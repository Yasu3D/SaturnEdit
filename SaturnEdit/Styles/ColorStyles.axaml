                    <SolidColorBrush x:Key="SliderWideTrackSecondary"             Color="#40000000" />
                    
                    <SolidColorBrush x:Key="SliderWideThumb"            Color="#AA7B41E0" />
                    <SolidColorBrush x:Key="SliderWideThumbPressed"     Color="#607B41E0" />
