<Styles xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:system="clr-namespace:System;assembly=System.Runtime">
    <Styles.Resources>
        <ResourceDictionary>
            <ResourceDictionary.ThemeDictionaries>
                
                <ResourceDictionary x:Key="Light">
                    <!--<SolidColorBrush x:Key="BackgroundPrimary" Color="#D4D4D4"/>
                    <SolidColorBrush x:Key="BackgroundSecondary" Color="#E1E1E1"/>
                    <SolidColorBrush x:Key="SeparatorPrimary" Color="#C6C6C6"/>
                    <SolidColorBrush x:Key="SeparatorSecondary" Color="#E1E1E1"/>
                    <SolidColorBrush x:Key="ForegroundPrimary" Color="#313131"/>
                    <SolidColorBrush x:Key="ForegroundSecondary" Color="#808080"/>
                    <SolidColorBrush x:Key="AccentColor" Color="#A44FE0"/>
                    
                    <SolidColorBrush x:Key="DockApplicationAccentBrushLow" Color="#C6C6C6"/> ~1~ Document Grip Selected @1@
                    <SolidColorBrush x:Key="DockApplicationAccentForegroundBrush">#313131</SolidColorBrush> ~1~ Window Text @1@
                    <SolidColorBrush x:Key="DockApplicationAccentBrushIndicator" Color="#007ACC"/> ~1~ DockTarget Overlay Color @1@
                    <SolidColorBrush x:Key="DockTargetPrimary" Color="#AA212121"/> ~1~ DockTarget Outline @1@
                    <SolidColorBrush x:Key="DockTargetSecondary" Color="#80212121"/> ~1~ DockTarget Fill @1@

                    <SolidColorBrush x:Key="DockThemeBorderLowBrush" Color="#C6C6C6" /> ~1~ Window Border @1@ ~1~ Document Grip Selected Unfocused @1@
                    <SolidColorBrush x:Key="DockThemeBackgroundBrush" Color="Transparent"/> ~1~ Window Background @1@
                    <SolidColorBrush x:Key="DockThemeAccentBrush" Color="Transparent" /> ~1~ Window Grip Selected @1@
                    <SolidColorBrush x:Key="DockThemeForegroundBrush" Color="#313131" /> ~1~ Document Text @1@
                    <system:Double x:Key="DockFontSizeNormal">12</system:Double>-->
                </ResourceDictionary>
                
                <ResourceDictionary x:Key="Dark">
                    <SolidColorBrush x:Key="BackgroundPrimary"   Color="#2B2B2B" />
                    <SolidColorBrush x:Key="BackgroundSecondary" Color="#1C1C1C" />
                    
                    <SolidColorBrush x:Key="SeparatorPrimary"   Color="#454545" />
                    <SolidColorBrush x:Key="SeparatorSecondary" Color="#1C1C1C" />

                    <SolidColorBrush x:Key="ForegroundPrimary"   Color="#ECEEF4" />
                    <SolidColorBrush x:Key="ForegroundSecondary" Color="#BBBBBB" />
                    <SolidColorBrush x:Key="ForegroundTertiary"  Color="#808080" />
                    <SolidColorBrush x:Key="ForegroundError"     Color="#DD0000"/>
                    
                    <SolidColorBrush x:Key="ButtonPrimaryPressed"     Color="#0AFFFFFF" />
                    <SolidColorBrush x:Key="ButtonPrimaryPointerOver" Color="#15FFFFFF" />
                    <SolidColorBrush x:Key="ButtonPrimaryDisabled"    Color="#1A000000" />
                    <SolidColorBrush x:Key="ButtonPrimaryChecked"     Color="#40FFFFFF" />
                    
                    <SolidColorBrush x:Key="ButtonSecondaryPressed"     Color="#40000000" />
                    <SolidColorBrush x:Key="ButtonSecondaryPointerOver" Color="#26000000" />
                    <SolidColorBrush x:Key="ButtonSecondaryDisabled"    Color="#10FFFFFF" />
                    <SolidColorBrush x:Key="ButtonSecondaryChecked"     Color="#40000000" />
                    
                    <SolidColorBrush x:Key="ButtonAccent"            Color="#FF7B41E0" />
                    <SolidColorBrush x:Key="ButtonAccentPressed"     Color="#887B41E0" />
                    <SolidColorBrush x:Key="ButtonAccentPointerOver" Color="#BB7B41E0" />
                    <SolidColorBrush x:Key="ButtonAccentDisabled"    Color="#707B41E0" />
                    <SolidColorBrush x:Key="ButtonAccentChecked"     Color="#FF7B41E0" />
                    
                    <SolidColorBrush x:Key="SliderWideTrackPrimary"               Color="#307B41E0" />
                    <SolidColorBrush x:Key="SliderWideTrackPrimaryPressed"        Color="#407B41E0" />
                    <SolidColorBrush x:Key="SliderWideTrackPrimaryPointerOver"    Color="#607B41E0" />
                    <SolidColorBrush x:Key="SliderWideTrackPrimaryBorder"         Color="#AA7B41E0" />
                    <SolidColorBrush x:Key="SliderWideTrackPrimaryDisabled"       Color="#40888888" />
                    <SolidColorBrush x:Key="SliderWideTrackPrimaryBorderDisabled" Color="#40DDDDDD" />
                    
                    <SolidColorBrush x:Key="SliderWideTrackSecondary"             Color="#40000000" />
                    
                    <SolidColorBrush x:Key="SliderWideThumb"            Color="#AA7B41E0" />
                    <SolidColorBrush x:Key="SliderWideThumbPressed"     Color="#607B41E0" />
                    <SolidColorBrush x:Key="SliderWideThumbPointerOver" Color="#CC7B41E0" />
                    <SolidColorBrush x:Key="SliderWideThumbDisabled"    Color="#40888888" />
                    
                    <SolidColorBrush x:Key="TextBoxBackground"            Color="#40000000" />
                    <SolidColorBrush x:Key="TextBoxBackgroundPointerOver" Color="#60000000" />
                    <SolidColorBrush x:Key="TextBoxBackgroundFocused"     Color="#70000000" />
                    <SolidColorBrush x:Key="TextBoxAccent"                Color="#FF7B41E0" />
                    

                    <!-- Legacy Dock Stuff, mostly overwritten by custom control themes. -->
                    <SolidColorBrush x:Key="DockApplicationAccentBrushLow" Color="#454545" /> <!-- Document Grip Selected -->
                    <SolidColorBrush x:Key="DockApplicationAccentForegroundBrush" Color="#D0D0D0" /> <!-- Window Text -->
                    <SolidColorBrush x:Key="DockApplicationAccentBrushIndicator" Color="#007ACC" /> <!-- DockTarget Overlay Color -->
                    <SolidColorBrush x:Key="DockTargetPrimary" Color="#AADEDEDE" /> <!-- DockTarget Outline -->
                    <SolidColorBrush x:Key="DockTargetSecondary" Color="#80DEDEDE" /> <!-- DockTarget Fill -->

                    <SolidColorBrush x:Key="DockThemeBorderLowBrush" Color="#454545" /> <!-- Window Border --> <!-- Document Grip Selected Unfocused -->
                    <SolidColorBrush x:Key="DockThemeBackgroundBrush" Color="Transparent" /> <!-- Window Background -->
                    <SolidColorBrush x:Key="DockThemeAccentBrush" Color="Transparent" /> <!-- Window Grip Selected -->
                    <SolidColorBrush x:Key="DockThemeForegroundBrush" Color="#D0D0D0" /> <!-- Document Text -->
                    
                    <system:Double x:Key="DockFontSizeNormal">12</system:Double>

                    <RadialGradientBrush x:Key="ChartEditorChromeGradient"
                                         RadiusX="225"
                                         RadiusY="225"
                                         Center="150, 20"
                                         GradientOrigin="200, 20">
                        <GradientStop Color="#30926BFF" Offset="0.0"/>
                        <GradientStop Color="#00926BFF" Offset="1.0"/>
                    </RadialGradientBrush>
                    
                    <RadialGradientBrush x:Key="StageEditorChromeGradient"
                                         RadiusX="225"
                                         RadiusY="225"
                                         Center="150, 20"
                                         GradientOrigin="200, 20">
                        <GradientStop Color="#30E46161" Offset="0.0"/>
                        <GradientStop Color="#00E46161" Offset="1.0"/>
                    </RadialGradientBrush>
                    
                    <RadialGradientBrush x:Key="CosmeticsEditorChromeGradient"
                                         RadiusX="225"
                                         RadiusY="225"
                                         Center="150, 20"
                                         GradientOrigin="200, 20">
                        <GradientStop Color="#30269EBB" Offset="0.0"/>
                        <GradientStop Color="#00269EBB" Offset="1.0"/>
                    </RadialGradientBrush>
                </ResourceDictionary>
                
            </ResourceDictionary.ThemeDictionaries>
        </ResourceDictionary>
    </Styles.Resources>
</Styles>