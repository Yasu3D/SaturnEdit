<Styles xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:system="clr-namespace:System;assembly=System.Runtime">
    <Styles.Resources>
        <ResourceDictionary>
            <ResourceDictionary.ThemeDictionaries>
                
                <ResourceDictionary x:Key="Light">
                    <SolidColorBrush x:Key="BackgroundPrimary"   Color="#EEEEEE" />
                    <SolidColorBrush x:Key="BackgroundSecondary" Color="#CCCCCC" />
                    
                    <SolidColorBrush x:Key="SeparatorPrimary"   Color="#999999" />
                    <SolidColorBrush x:Key="SeparatorSecondary" Color="#EEEEEE" />

                    <SolidColorBrush x:Key="ForegroundPrimary"   Color="#FF000000" />
                    <SolidColorBrush x:Key="ForegroundSecondary" Color="#80000000" />
                    <SolidColorBrush x:Key="ForegroundTertiary"  Color="#40000000" />
                    <SolidColorBrush x:Key="ForegroundError"     Color="#DDFF0000" />
                    <SolidColorBrush x:Key="ForegroundWarning"   Color="#DDFFA800" />
                    
                    <SolidColorBrush x:Key="ButtonPrimaryPressed"     Color="#0AFFFFFF" />
                    <SolidColorBrush x:Key="ButtonPrimaryPointerOver" Color="#15FFFFFF" />
                    <SolidColorBrush x:Key="ButtonPrimaryDisabled"    Color="#1A000000" />
                    <SolidColorBrush x:Key="ButtonPrimaryChecked"     Color="#30FFFFFF" />
                    
                    <SolidColorBrush x:Key="ButtonSecondaryPressed"     Color="#40000000" />
                    <SolidColorBrush x:Key="ButtonSecondaryPointerOver" Color="#26000000" />
                    <SolidColorBrush x:Key="ButtonSecondaryDisabled"    Color="#10FFFFFF" />
                    <SolidColorBrush x:Key="ButtonSecondaryChecked"     Color="#40000000" />
                    
                    <SolidColorBrush x:Key="ButtonAccent"            Color="#FF7B41E0" />
