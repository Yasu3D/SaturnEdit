                    <ComboBoxItem Content="{DynamicResource Settings.Rendering.EffectVisibility.AlwaysOff}" />
                    <ComboBoxItem Content="{DynamicResource Settings.Rendering.EffectVisibility.OnlyWhenPlaying}" />
                    <ComboBoxItem Content="{DynamicResource Settings.Rendering.EffectVisibility.OnlyWhenPaused}" />
                </ComboBox>
            </Grid>
            
            <Grid ColumnDefinitions="250,*"
                  Margin="0,0,0,10">
