                    <ComboBoxItem Content="{DynamicResource Settings.Rendering.GuideLineType.B}" />
                    <ComboBoxItem Content="{DynamicResource Settings.Rendering.GuideLineType.C}" />
                    <ComboBoxItem Content="{DynamicResource Settings.Rendering.GuideLineType.D}" />
                    <ComboBoxItem Content="{DynamicResource Settings.Rendering.GuideLineType.E}" />
                    <ComboBoxItem Content="{DynamicResource Settings.Rendering.GuideLineType.F}" />
                    <ComboBoxItem Content="{DynamicResource Settings.Rendering.GuideLineType.G}" />
                    <ComboBoxItem Content="{DynamicResource Settings.Rendering.GuideLineType.None}" />
                </ComboBox>
            </Grid>
            
            <Grid ColumnDefinitions="250,*"
                  Margin="0,0,0,10">
                <TextBlock Grid.Column="0"
                           Text="{DynamicResource Settings.Rendering.JudgementLineColor}"
                           VerticalAlignment="Center"
                           Margin="0,0,10,0" />
                <ComboBox Grid.Column="1"
                          Name="ComboBoxJudgementLineColor"
                          HorizontalAlignment="Left"
                          Width="170"
                          SelectedIndex="0"
                          SelectionChanged="ComboBoxJudgementLineColor_OnSelectionChanged">
                    <ComboBoxItem Content="{DynamicResource Settings.Rendering.JudgementLineColor.Version1}" />
                    <ComboBoxItem Content="{DynamicResource Settings.Rendering.JudgementLineColor.Version2}" />
                    <ComboBoxItem Content="{DynamicResource Settings.Rendering.JudgementLineColor.Version3}" />
                    <ComboBoxItem Content="{DynamicResource Settings.Rendering.JudgementLineColor.Greyscale}" />
                </ComboBox>
            </Grid>
            
            <Grid ColumnDefinitions="250,*"
                  Margin="0,20,0,10">
