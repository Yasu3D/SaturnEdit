<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d"
             d:DesignWidth="650"
             d:DesignHeight="610"
             x:Class="SaturnEdit.Windows.Dialogs.Settings.Tabs.SettingsRenderingView">
    <ScrollViewer Background="{DynamicResource BackgroundPrimary}">
        <StackPanel Margin="20">
            <!-- Performance -->
            <Grid ColumnDefinitions="Auto,*"
                  Margin="0,0,0,20">
                <TextBlock Grid.Column="0"
                           Text="{DynamicResource Settings.Rendering.Performance}"
                           Margin="0,0,20,0" />
                <Rectangle Grid.Column="1"
                           HorizontalAlignment="Stretch"
                           VerticalAlignment="Center"
                           Height="1"
                           Fill="{DynamicResource SeparatorPrimary}" />
            </Grid>
            
            <StackPanel Orientation="Horizontal"
                        Margin="0,0,0,10">
                <CheckBox Name="CheckBoxLowPerformanceMode"
                          IsChecked="False" 
                          IsCheckedChanged="CheckBoxLowPerformanceMode_OnIsCheckedChanged"/>
                <TextBlock Text="{DynamicResource Settings.Rendering.LowPerformanceMode}"
                           VerticalAlignment="Center" />
            </StackPanel>

            <!-- 3D View -->
            <Grid ColumnDefinitions="Auto,*"
                  Margin="0,40,0,20">
                <TextBlock Grid.Column="0"
                           Text="{DynamicResource Settings.Rendering.3DView}"
                           Margin="0,0,20,0" />
                <Rectangle Grid.Column="1"
                           HorizontalAlignment="Stretch"
                           VerticalAlignment="Center"
                           Height="1"
                           Fill="{DynamicResource SeparatorPrimary}" />
            </Grid>

            <Grid ColumnDefinitions="250,*"
                  Margin="0,0,0,10">
                <TextBlock Grid.Column="0"
                           Text="{DynamicResource Settings.Rendering.NoteSpeed}"
                           VerticalAlignment="Center"
                           Margin="0,0,10,0" />
                <NumericUpDown Grid.Column="1"
                               Name="NumericUpDownNoteSpeed"
                               HorizontalAlignment="Left"
                               Width="170"
                               Minimum="1.0"
                               Maximum="6.0"
                               Increment="0.1"
                               Value="3.0"
                               FormatString="F1" 
                               ValueChanged="NumericUpDownNoteSpeed_OnValueChanged"/>
            </Grid>

            <Grid ColumnDefinitions="250,*"
                  Margin="0,0,0,10">
                <TextBlock Grid.Column="0"
                           Text="{DynamicResource Settings.Rendering.BackgroundDim}"
                           VerticalAlignment="Center"
                           Margin="0,0,10,0" />
                <ComboBox Grid.Column="1"
                          Name="ComboBoxBackgroundDim"
                          HorizontalAlignment="Left"
                          Width="170"
                          SelectedIndex="0"
                          SelectionChanged="ComboBoxBackgroundDim_OnSelectionChanged">
                    <ComboBoxItem Content="{DynamicResource Settings.Rendering.BackgroundDim.Level0}" />
                    <ComboBoxItem Content="{DynamicResource Settings.Rendering.BackgroundDim.Level1}" />
                    <ComboBoxItem Content="{DynamicResource Settings.Rendering.BackgroundDim.Level2}" />
                    <ComboBoxItem Content="{DynamicResource Settings.Rendering.BackgroundDim.Level3}" />
                    <ComboBoxItem Content="{DynamicResource Settings.Rendering.BackgroundDim.Level4}" />
                </ComboBox>
            </Grid>
            
            <Grid ColumnDefinitions="250,*"
                  Margin="0,20,0,10">
