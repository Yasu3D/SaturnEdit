<Styles xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:system="clr-namespace:System;assembly=System.Runtime">
    <Styles.Resources>
        <ResourceDictionary>
            <ResourceDictionary.ThemeDictionaries>
                
                <ResourceDictionary x:Key="Light">
                    <!--<SolidColorBrush x:Key="BackgroundPrimary" Color="#D4D4D4"/>
                    <SolidColorBrush x:Key="BackgroundSecondary" Color="#E1E1E1"/>
                    <SolidColorBrush x:Key="SeparatorPrimary" Color="#C6C6C6"/>
                    <SolidColorBrush x:Key="SeparatorSecondary" Color="#E1E1E1"/>
                    <SolidColorBrush x:Key="ForegroundPrimary" Color="#313131"/>
                    <SolidColorBrush x:Key="ForegroundSecondary" Color="#808080"/>
                    <SolidColorBrush x:Key="AccentColor" Color="#A44FE0"/>
                    
                    <SolidColorBrush x:Key="DockApplicationAccentBrushLow" Color="#C6C6C6"/> ~1~ Document Grip Selected @1@
                    <SolidColorBrush x:Key="DockApplicationAccentForegroundBrush">#313131</SolidColorBrush> ~1~ Window Text @1@
                    <SolidColorBrush x:Key="DockApplicationAccentBrushIndicator" Color="#007ACC"/> ~1~ DockTarget Overlay Color @1@
                    <SolidColorBrush x:Key="DockTargetPrimary" Color="#AA212121"/> ~1~ DockTarget Outline @1@
                    <SolidColorBrush x:Key="DockTargetSecondary" Color="#80212121"/> ~1~ DockTarget Fill @1@

                    <SolidColorBrush x:Key="DockThemeBorderLowBrush" Color="#C6C6C6" /> ~1~ Window Border @1@ ~1~ Document Grip Selected Unfocused @1@
                    <SolidColorBrush x:Key="DockThemeBackgroundBrush" Color="Transparent"/> ~1~ Window Background @1@
                    <SolidColorBrush x:Key="DockThemeAccentBrush" Color="Transparent" /> ~1~ Window Grip Selected @1@
                    <SolidColorBrush x:Key="DockThemeForegroundBrush" Color="#313131" /> ~1~ Document Text @1@
                    <system:Double x:Key="DockFontSizeNormal">12</system:Double>-->
                </ResourceDictionary>
                
                <ResourceDictionary x:Key="Dark">
                    <SolidColorBrush x:Key="BackgroundPrimary" Color="#2B2B2B"/>
                    <SolidColorBrush x:Key="BackgroundSecondary" Color="#1C1C1C"/>
                    <SolidColorBrush x:Key="SeparatorPrimary" Color="#454545"/>
                    <SolidColorBrush x:Key="SeparatorSecondary" Color="#1C1C1C"/>
                    <SolidColorBrush x:Key="ForegroundPrimary" Color="#D0D0D0"/>
                    <SolidColorBrush x:Key="ForegroundSecondary" Color="#AAAAAA"/>
                    <SolidColorBrush x:Key="ForegroundTertiary" Color="#808080"/>
                    <SolidColorBrush x:Key="AccentColor" Color="#A44FE0"/>
                    
                    
                    
                    
                    <SolidColorBrush x:Key="DockApplicationAccentBrushLow" Color="#454545"/> <!-- Document Grip Selected -->
                    <SolidColorBrush x:Key="DockApplicationAccentForegroundBrush">#D0D0D0</SolidColorBrush> <!-- Window Text -->
                    <SolidColorBrush x:Key="DockApplicationAccentBrushIndicator" Color="#007ACC"/> <!-- DockTarget Overlay Color -->
                    <SolidColorBrush x:Key="DockTargetPrimary" Color="#AADEDEDE"/> <!-- DockTarget Outline -->
                    <SolidColorBrush x:Key="DockTargetSecondary" Color="#80DEDEDE"/> <!-- DockTarget Fill -->

                    <SolidColorBrush x:Key="DockThemeBorderLowBrush" Color="#454545" /> <!-- Window Border --> <!-- Document Grip Selected Unfocused -->
                    <SolidColorBrush x:Key="DockThemeBackgroundBrush" Color="Transparent"/> <!-- Window Background -->
                    <SolidColorBrush x:Key="DockThemeAccentBrush" Color="Transparent" /> <!-- Window Grip Selected -->
                    <SolidColorBrush x:Key="DockThemeForegroundBrush" Color="#D0D0D0" /> <!-- Document Text -->
                    <system:Double x:Key="DockFontSizeNormal">12</system:Double>
                </ResourceDictionary>
                
            </ResourceDictionary.ThemeDictionaries>
        </ResourceDictionary>
    </Styles.Resources>
</Styles>